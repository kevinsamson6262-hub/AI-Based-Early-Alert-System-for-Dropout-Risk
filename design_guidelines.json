  "instructions_to_main_agent": {
    "setup": [
      "Install fonts: Manrope and IBM Plex Sans.",
      "Update tailwind.config.js with the custom color palette.",
      "Install 'framer-motion' and 'recharts' if not present.",
      "Ensure 'lucide-react' is available."
    ],
    "implementation_rules": [
      "DO NOT use default blue colors. Use the defined Primary (#C05621) and Secondary (#475569).",
      "DO NOT use 'Inter' font. Use Manrope for headings.",
      "Create a 'Layout' component that handles the sidebar/header structure for the dashboard.",
      "Use 'Card' components for all dashboard widgets, but ensure they have the 'rounded-xl' and soft shadow styles.",
      "For the Risk Dashboard, use the 'Control Room' grid layout.",
      "Ensure all interactive elements have 'data-testid' attributes."
    ],
    "universal_guidelines": [
      "Avoid generic centered layouts.",
      "Use glassmorphism for sticky headers.",
      "Add micro-interactions (hover states) to all buttons and cards.",
      "Use 'sonner' for toast notifications."
    ]
  }
}